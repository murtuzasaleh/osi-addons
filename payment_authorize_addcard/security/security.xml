<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="payment_transaction_employee_rule" model="ir.rule">
        <field name="name">Access all payment transaction</field>
        <field name="model_id" ref="payment.model_payment_transaction"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
    </record>
    <record id="payment_token_employee_rule" model="ir.rule">
        <field name="name">Access all payment tokens</field>
        <field name="model_id" ref="payment.model_payment_token"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>
